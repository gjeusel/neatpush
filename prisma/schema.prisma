generator client {
  provider  = "prisma-client-py"
  interface = "asyncio"
}

datasource db {
    provider = "sqlite"
    url      = "file:./db.sqlite"
    // url      = env("DATABASE_URL")
}

model MangaChapter {
  id         String @id @default(cuid())
  num String
  timestamp  DateTime @default(now())
}
